numero3.mostrarvalor();
if (numero3.esPrimo()) Console.WriteLine("Es primo");
else Console.WriteLine("No es primo");
Console.WriteLine(numero3.getValor());
NumeroNatural numOrden = new NumeroNatural(530241);
